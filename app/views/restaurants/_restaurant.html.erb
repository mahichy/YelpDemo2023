<div id="<%= dom_id restaurant %>">
  
   <% if @restaurant.image_url %>
    <%= image_tag @restaurant.image_url %>
 <% end %>
  
  
  <p>
    <strong>Name:</strong>
    <%= restaurant.name %>
  </p>

  <p>
    <strong>Address:</strong>
    <%= restaurant.address %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= restaurant.phone %>
  </p>

  <p>
    <strong>Website:</strong>
    <%= restaurant.website %>
  </p>

 

</div>
